    "core-js": "^3.4.2",
    "cuid": "^2.1.4",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-ws": "^4.0.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "nanoid": "^1.3.4",
    "spectrum-colorpicker": "^1.8.0",
    "sqlite3": "4.0.6",
    "tinycolor2": "^1.4.1",
    "vue": "^2.5.22",
    "vue-router": "^3.0.1",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
