{
  "name": "rpnow",
  "private": true,
  "scripts": {
    "start": "node src/index.js",
    "start-dev": "cross-env VUE_DEV=true DEBUG=rpnow nodemon src/index.js --ext js,json,vue,css",
    "lint": "eslint src/**/*.js src/*.js",
    "test": "ava --watch",
    "build": "./build.sh",
    "grant-node-low-ports": "sudo sh -c \"setcap 'cap_net_bind_service=+ep' $(which node)\""
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "axios": "^0.18.0",
    "busboy": "^0.3.0",
    "camelcase": "^5.0.0",
    "cuid": "^2.1.4",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-vue": "^5.8.2",
    "greenlock-express": "^2.6.7",
    "ini": "^1.3.5",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "nanoid": "^1.3.4",
    "nconf": "^0.10.0",
    "sqlite3": "4.0.6",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "ava": "1.0.0-rc.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.9",
    "pkg": "^4.3.5",
    "supertest": "^3.3.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ]
  },
  "bin": "src/index.js",
  "pkg": {
    "assets": [
      "src/views/**",
      "src/static/**"
    ]
  }
}
