{
  "name": "rpnow",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "parcel build src/index.html",
    "start": "npm run build && node index.js",
    "start-dev": "cross-env DEBUG=rpnow nodemon index.js --ext js,json,vue,css",
    "test": "ava --watch"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "busboy": "^0.3.0",
    "cuid": "^2.1.4",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-ws": "^4.0.0",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "nanoid": "^1.3.4",
    "parcel": "^1.12.4",
    "sqlite3": "4.0.6",
    "vue": "^2.5.22",
    "vue-hot-reload-api": "^2.3.4",
    "vue-router": "^3.0.1",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@vue/babel-preset-app": "^4.0.5",
    "@vue/component-compiler-utils": "^3.0.2",
    "autoprefixer": "^9.7.2",
    "ava": "1.0.0-rc.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-vue": "^5.0.0",
    "nodemon": "^2.0.1",
    "supertest": "^3.3.0",
    "vue-template-compiler": "^2.6.10"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ]
  },
  "bin": "src/index.js",
  "pkg": {
    "assets": [
      "src/views/**",
      "src/static/**"
    ]
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
